      float: left;
      margin: 0px !important;
      border-top-left-radius: 50px;
      border-bottom-left-radius: 50px;
      background: #f4f4f4 !important;
      font-size: 18px !important;
      padding: 0px 20px;
      height: 45px;
      width: 70%;
      max-width: 500px;

      //  Tablet
      @media screen and (min-width:$medium) and (max-width:$large) {
        max-width: 200px;
      }
      

  align-items:center;
  background: #fff !important;

  //  Tablet
  @media screen and (min-width:$medium) and (max-width:$large) {
    justify-content: space-between;
  }

  .menu-text {
    width: 30%;
    padding:0px !important;
  }

  .head-search  {
    list-style:none;
    width: 60%;
    @media screen and (max-width:$medium) {
      width: 100%; 
    }

    .search_form {
      display: block;
      width: 100%;
	  position: relative;
      //  Tablet
      @media screen and (min-width:$medium) and (max-width:$large) {
        @include flexbox;
        justify-content: center;
      }      
      .fa-search {
        display: none;
      }
      @media screen and (max-width:$medium) {
      	.fa-search {
          display: block;
          position:absolute;
          left: 9.5%;

  {% include 'gsf-conversion-pixels' %}


  {%- assign yotpo_offload_content = shop.metafields.yotpo.yotpo_offload_content %}
  {%- assign time_now = 'now' | date: '%s' %}
  {%- assign yotpo_live_time = shop.metafields.yotpo.yotpo_live | date: '%s' %}
  {%- assign diffSeconds = time_now | minus: yotpo_live_time %}
  {%- if yotpo_offload_content == 'yes' and diffSeconds < 86400 -%}
  {{ 'yotpo-full-css.css' | asset_url | stylesheet_tag }}
  {%- endif -%}

  {% render 'gsf-conversion-pixels' %}
  {% include 'scapp-customerinfo' %} 
  
  {% include 'website-schema' %}
  {% if template == "index" %}{% include 'localbusiness-schema' %}{% endif %}
  {% include 'organization-schema' %}
  {% comment %}{% include 'prof-service-schema' %}{% endcomment %}
  {% include 'breadcrumb-schema' %}

  <!--- Start of [GetEmails.com|http://GetEmails.com] Code ---><script async src="https://s3-us-west-2.amazonaws.com/files.getemails.com/account/4X0HW4E/source/getemails.js"></script><!--- End of [GetEmails.com|http://GetEmails.com] Code --->
</head>

<body id="{{ page_title | handle }}" class="template-{{ template.name | handle }} {{ settings.body_classes }}">
  
<!-- Aftership Code -->
<div id="as-root"></div><script>(function(e,t,n){var r,i=e.getElementsByTagName(t)[0];if(e.getElementById(n))return;
r=e.createElement(t);r.id=n;r.src="//button.aftership.com/all.js";i.parentNode.insertBefore(r,i)})(document,"script","aftership-jssdk")</script>
<!-- End Aftership Code -->

    {% comment %}off-canvas UI drawers{% endcomment %}
    {% include 'mobile-nav' %}
    {% include 'cart-elements' %}
  
  {% unless template contains "meat" %}
  <div class="off-canvas-content" data-off-canvas-content>

    <a class="in-page-link visually-hidden skip-link" href="#MainContent">{{ 'general.accessibility.skip_to_content' | t }}</a>
  
      {% section 'header' %}
      
      <div class="mega-menu-container">
      
        {% if settings.mega_menu_1 %}
          {% section 'mega-menu-1' %}
        {% endif %}

        {% if settings.mega_menu_2 %}
          {% section 'mega-menu-2' %}
        {% endif %}

        {% if settings.mega_menu_3 %}
          {% section 'mega-menu-3' %}
        {% endif %}

        {% if settings.mega_menu_4 %}
          {% section 'mega-menu-4' %}
        {% endif %}

        {% if settings.mega_menu_5 %}
          {% section 'mega-menu-5' %}
        {% endif %}

        {% if settings.mega_menu_6 %}
          {% section 'mega-menu-6' %}
        {% endif %}

      </div>
    {% endunless %}
    
      {% include 'page-handle' %}  
      <main role="main" id="MainContent" class="{{ template.name | handle }}-{{page_handle}}">
          {{ content_for_layout }}        
      </main>
      
      {% unless template contains "meat" %}
        {% if settings.pricing_rules %}
          {% section 'pricing-rules' %}
        {% endif %}
      
        {% section 'footer' %}

        {% if settings.pricing_rules %}
          {% section 'pricing-rules' %}
        {% endif %}

    </div>
      {% endunless %}
      
    {% include 'footer-scripts' %} 
	
	{% include 'iwish_v2' with 'iwishfooter' %}


  {% include 'elevar-body-end' %}
<!-- This line added automatically by JSON-LD for SEO. -->{% include 'jsonld-for-seo' %}
</body>
</html>

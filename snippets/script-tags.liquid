
<script>

    function loadJs(url, callback) {
        var e = document.createElement("script");
        e.src = url;
        e.type="text/javascript";
        e.addEventListener('load', callback);
        document.getElementsByTagName("head")[0].appendChild(e);
    }

	{%- if template.name == 'product' -%}
		load20(100);
		load21(100);
	{%- elsif template == 'cart.quote' -%}
		load20(100);
		load21(150);
    {%- elsif template == 'collection.quickorder' -%}
        /*load20(100);
        load21(100);*/
    {%- elsif template == 'article' -%}
        load20(200);
        load21(200);
    {%- elsif layout == 'theme' and template != 'cart' -%}
		load20(100);
        load21(1700);
    {%- elsif template == 'cart' -%}
        load20(100);
        load21(150);
	{%- endif -%}


	{%- if template.name == 'collection' or template.name == 'index' -%}
		load22(4000);
    {%- elsif template.name == 'product' -%}
        load22(1000);
    {%- endif -%}

	{%- if template.name == 'collection' -%}
		load23(3000);
	{%- endif -%}

	{%- if template.name == 'gift_card' -%}
		load24(100);
	{%- endif -%}

	{%- if template.name == 'index' -%}
		load25(3000);
	{%- endif -%}

	{%- if template.name == 'product' -%}
		load26(750);
	{%- endif -%}

	{%- if template.name == 'addresses' -%}
		load27(300);
	{%- endif -%}

	{%- if template.name == 'login' -%}
		load28(10);
	{%- endif -%}


	function load20(time) {
		setTimeout(function() {
